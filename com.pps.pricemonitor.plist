<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.pps.pricemonitor</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>scraper_stealth.py</string>
    </array>
    <key>WorkingDirectory</key>
    <string>__INSTALL_PATH__</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>8</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>__INSTALL_PATH__/data/cron.log</string>
    <key>StandardErrorPath</key>
    <string>__INSTALL_PATH__/data/cron_error.log</string>
    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>
